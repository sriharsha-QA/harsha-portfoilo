{
  "version": 2,
  "builds": [
    {
      "src": "build/client/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "build/server/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "AWS_REGION": "us-east-1"
  }
}
